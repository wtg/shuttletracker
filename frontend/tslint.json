{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": false,
    "no-var-requires": false,
    "max-line-length": false,
    "prefer-for-of": false,
    "variable-name": false,
    "no-empty": false,
    "max-classes-per-file": false,
    "no-bitwise": false,
    "no-console": false
  }
}
